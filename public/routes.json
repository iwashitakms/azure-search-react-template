{
    "routes": [
      {
        "route": "/logout",
        "serve": "/.auth/logout"
      },
      {
        "route": "/login",
        "serve": "/.auth/login/aad"
      },
      {
        "route": "/.auth/login/github",
        "statusCode": "404"
      },
      {
        "route": "/.auth/login/google",
        "statusCode": "404"
      },
      {
        "route": "/.auth/login/facebook",
        "statusCode": "404"
      },
      {
        "route": "/.auth/login/twitter",
        "statusCode": "404"
      },
      {
        "route": "/home",
        "serve": "/",
        "statusCode": 301,
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/api/document",
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/api/document/update"
      },
      { 
        "route": "/document",
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/search" ,
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/list" ,
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/api/search",
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/api/lookup",
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/api/suggest",
        "allowedRoles": ["reader"]
      },
      { 
        "route": "/details/*" ,
        "allowedRoles": ["reader"]
      },
      {
        "route": "/*",
        "serve": "/",
        "statusCode": 200,
        "allowedRoles": ["reader"]
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthenticated",
        "statusCode": 302,
        "serve": "/login"
      },
      {
        "errorType": "NotFound",
        "serve": "/index.html",
        "statusCode": 200
      }
    ]
  }